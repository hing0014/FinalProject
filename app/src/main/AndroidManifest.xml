<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.finalProject"
    android:name="android.hardware.camera"
    android:required="false"
    tools:ignore="ImpliedTouchscreenHardware,MissingLeanbackLauncher,MissingLeanbackSupport">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/custom"
        android:label="@string/app_name"
        android:roundIcon="@drawable/magnify"
        android:supportsRtl="true"
        android:windowSoftInputMode="adjustResize"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.finalProject.Navigation" />
        <activity android:name="com.example.finalProject.Recipes" />
        <activity android:name="com.example.finalProject.Audio" />
        <activity android:name="com.example.finalProject.Covid" />
        <activity android:name="com.example.finalProject.TicketMaster" />
<!--        <activity android:name="com.example.finalProject.MainActivity" />-->
          <activity android:name="com.example.finalProject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>